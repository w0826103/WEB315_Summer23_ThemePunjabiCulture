@page "/punjabiCulture"

@code{
    string[] items = new string[]{
        "dress","food","festivals","religion","history"
    };
    private int data = 0;
    private void newData(int newData){
        data=newData;
    }
}
@foreach(var culture in items){
    <p> @culture</p>
}
<input type="number" value="5"/>
@for(int p =0; p <=data ; p++)
{
    if(p >=0 && p < 5){
        <p> @items[p]</p>
    }
    
    }
